@Article{smallchu03,
  author   = {Kenneth A. Small and Xuehao Chu},
  journal  = {Journal of Transport Economics and Policy},
  title    = {{Hypercongestion}},
  year     = {2003},
  month    = {September},
  number   = {3},
  pages    = {319-352},
  volume   = {37},
  abstract = {The standard economic model for analysing traffic congestion incorporates a relationship between speed and traffic flow. Empirical measurements indicate a region, known as hypercongestion, in which speed increases with flow. We argue that this relationship is unsuitable as a supply curve for equilibrium analysis because observed hypercongestion occurs as a response to transient demand fluctuations. We then present tractable models for handling such fluctuations, both for a straight uniform highway and for a dense street network such as in a central business district (CBD). For the CBD model, we consider both exogenous and endogenous time patterns for demand, and we make use of an empirical speed-density relationship for Dallas, Texas, to characterise hypercongested conditions. The CBD model is adaptable to any situation where accumulation of work to be processed becomes such a hindrance as to reduce outflow. © The London School of Economics and the University of Bath 2003},
  url      = {https://ideas.repec.org/a/tpe/jtecpo/v37y2003i3p319-352.html},
}

@Article{wardman98,
  author    = {Mark Wardman},
  journal   = {Journal of Transport Economics and Policy},
  title     = {{The Value of Travel Time: A Review of British Evidence}},
  year      = {1998},
  number    = {3},
  pages     = {285--316},
  volume    = {32},
  abstract  = {This paper reviews British evidence regarding the value of travel time available from models developed since 1980. There are two main aspects to the review. First, a regression model has been developed to explain variations in the value of time across studies. Second, the author reports on a review of variations in the value of time within specific studies. The review also places the research in historical perspective, with reference to work conducted elsewhere. /// In diesem Aufsatz werden Ergebnisse britischer Modelle einer kritischen Betrachtung unterzogen, die sich mit dem Wert der Reisezeit beschäftigen und nach 1980 entstanden sind. Zwei Punkte bilden dabei den Kern der Analyse: Erstens wird in einer Regressionsanalyse versucht, die Variation des Werts der Zeit über verschiedene Studien hinweg zu erklären. Eine Betrachtung dieser Variation innerhalb ausgewählter Studien bildet dann den zweiten Schwerpunkt. Der Aufsatz ordnet die Forschung in einen historischen Kontext ein, und nimmt Bezug auf andere Arbeiten zum Thema. /// Cet article passe en revue les constatations de la Grande-Bretagne en ce qui concerne la valeur du temps de trajet, disponibles à partir de modèles développés depuis 1980. Cet examen se concentre autour de deux axes. Premièrement, un modèle de régression fut constitué afin d'expliquer les fluctuations de la valeur du temps observées dans diverses études. Deuxièmement, l'auteur rapporte sur une enquête de variations de la valeur du temps au sein d'études spécifiques. En outre, l'enquête place cette recherche dans une perspective historique, en se référant à des travaux dirigés dans d'autres pays. /// Este artículo revisa la evidencia británica sobre el valor del tiempo de viaje en modelos desarrollados desde 1980. La revista tiene dos aspectos principales. Primero, se desarrolla un modelo de regresión para explicar las variaciones en el valor del tiempo entre estudios. Segundo, el autor presenta una revista de las variaciones en el valor del tiempo en estudios concretos. La revista también sitúa la investigación en su perspectiva histórica, con referencia al rabajo desarrollado en otros lugares.},
  publisher = {[London School of Economics, University of Bath, London School of Economics and University of Bath, London School of Economics and Political Science]},
  url       = {http://www.jstor.org/stable/20053775},
}

@Article{couture2018,
  author   = {Couture, Victor and Duranton, Gilles and Turner, Matthew A.},
  journal  = {The Review of Economics and Statistics},
  title    = {Speed},
  year     = {2018},
  number   = {4},
  pages    = {725-739},
  volume   = {100},
  abstract = {We investigate determinants of driving speed in large U.S. cities. We first estimate city-level supply functions for travel in an econometric framework where the supply and demand for travel are explicit. These estimations allow us to calculate an index of driving speed and to rank cities by driving speed. Our data suggest that a congestion tax of about 3.5 cents per kilometer yields welfare gains of about \$30 billion per year, that centralized cities are slower, that cities with ring roads are faster, and that the provision of automobile travel in cities is subject to decreasing returns to scale.},
  doi      = {10.1162/rest\_a\_00744},
  eprint   = {https://doi.org/10.1162/rest_a_00744},
  url      = {https://doi.org/10.1162/rest_a_00744},
}

